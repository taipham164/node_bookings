# React Bricks Configuration
# Get your App ID and API Key from https://dashboard.reactbricks.com
NEXT_PUBLIC_REACT_BRICKS_APP_ID=YOUR_APP_ID
REACT_BRICKS_API_KEY=YOUR_API_KEY

# Optional: Set to 'production' for production builds
NODE_ENV=development

# Backend API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:3001

# Shop Configuration
NEXT_PUBLIC_SHOP_ID=your-shop-id-here

# Square Web Payments SDK Configuration
# Get these from your Square Developer Dashboard: https://developer.squareup.com/apps
NEXT_PUBLIC_SQUARE_APP_ID=your-square-app-id
NEXT_PUBLIC_SQUARE_LOCATION_ID=your-square-location-id
NEXT_PUBLIC_SQUARE_ENVIRONMENT=sandbox
