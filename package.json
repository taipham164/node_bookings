{
  "name": "tyler-platform",
  "version": "1.0.0",
  "private": true,
  "description": "Tyler Platform - Monorepo with Square Bookings API",
  "scripts": {
    "dev:legacy": "pnpm --filter legacy-express start",
    "dev:backend": "pnpm --filter backend start:dev",
    "dev:tyler-site": "pnpm --filter tyler-site dev",
    "build:backend": "pnpm --filter backend build",
    "build:tyler-site": "pnpm --filter tyler-site build",
    "start:backend": "pnpm --filter backend start",
    "start:tyler-site": "pnpm --filter tyler-site start",
    "install:backend": "pnpm install --filter backend",
    "test": "pnpm --filter legacy-express test",
    "test:backend": "pnpm --filter backend test",
    "lint": "pnpm --filter legacy-express lint",
    "lint:backend": "pnpm --filter backend lint",
    "lint:tyler-site": "pnpm --filter tyler-site lint"
  },
  "engines": {
    "node": ">=10",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.12.4"
}
